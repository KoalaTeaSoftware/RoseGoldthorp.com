
RewriteEngine on
# redirects for the API
# Take sledghammer and pass everything in
# this should catch any number of specifiers that are given
RewriteCond %{HTTP_HOST} ^rosegoldthorp\.com$ [OR]
RewriteCond %{HTTP_HOST} ^dev.rosegoldthorp\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.rosegoldthorp\.com$
RewriteCond %{REQUEST_URI} !^/[0-9]+\..+\.cpaneldcv$
RewriteCond %{REQUEST_URI} !^/\.well-known/pki-validation/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
#
RewriteRule ^films[0-9a-z\-\/]*$ showFilms.html [NC,L]
RewriteRule ^film[0-9a-z\-\/]*$ showFilmDetail.html [NC,L]
<IfModule mod_headers.c>
	Header set Cache-Control "no-cashe, no-store, must-revalidate"
	Header set Pragma "no-cache"
#	Header set Expires 0
</IfModule>

# php -- BEGIN cPanel-generated handler, do not edit
# Set the “ea-php72” package as the default “PHP” programming language.
<IfModule mime_module>
  AddHandler application/x-httpd-ea-php72___lsphp .php .php7 .phtml
</IfModule>
# php -- END cPanel-generated handler, do not edit
